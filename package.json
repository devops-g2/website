{

  "name": "website",

  "private": true,

  "version": "0.0.0",

  "type": "module",

  "extensions": [

    "js",

    "jsx",

    "ts",

    "tsx",

    "mjs"

  ],

  "scripts": {

    "type": "module",

    "test": "jest",

    "dev": "vite --host",

    "build": "vite build",

    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",

    "format": "prettier --check \"src/**/*.{js,jsx,json,md,ts,tsx}\" \"contexts/**/*.{js,jsx,ts,tsx}\" \"components/**/*.{js,jsx,ts,tsx}\" \"pages/**/*.{js,jsx,ts,tsx}\" \"services/**/*.{js,jsx,ts,tsx}\"",

    "format:write": "prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx}\" \"contexts/**/*.{js,jsx,ts,tsx}\" \"components/**/*.{js,jsx,ts,tsx}\" \"pages/**/*.{js,jsx,ts,tsx}\" \"services/**/*.{js,jsx,ts,tsx}\"",

    "preview": "vite preview"

  },

  "dependencies": {

    "@fortawesome/fontawesome-svg-core": "^6.4.2",

    "@fortawesome/free-solid-svg-icons": "^6.4.2",

    "@fortawesome/react-fontawesome": "^0.2.0",

    "@types/node-fetch": "^2.6.11",

    "react": "^18.2.0",

    "react-dom": "^18.2.0",

    "react-router-dom": "^6.16.0"

  },

  "devDependencies": {

    "@babel/core": "^7.23.7",

    "@babel/preset-env": "^7.23.8",

    "@babel/preset-flow": "^7.23.3",

    "@types/jest": "^29.5.11",

    "@types/react": "^18.2.15",

    "@types/react-dom": "^18.2.7",

    "@vitejs/plugin-react": "^4.0.3",

    "babel-jest": "^29.7.0",

    "eslint": "^8.56.0",

    "eslint-plugin-react": "^7.32.2",

    "eslint-plugin-react-hooks": "^4.6.0",

    "eslint-plugin-react-refresh": "^0.4.3",

    "fetch-mock": "^9.11.0",

    "fetch-mock-jest": "^1.5.1",

    "jest": "^29.7.0",

    "jest-fetch-mock": "^3.0.3",

    "node-fetch": "^2.7.0",

    "prettier": "^3.2.4",

    "ts-jest": "^29.1.1",

    "vite": "^4.4.5"

  },

  "jest": {

    "testEnvironment": "node",

    "moduleFileExtensions": [

      "js",

      "jsx",

      "json",

      "ts",

      "tsx",

      "node"

    ],

    "roots": [

      "<rootDir>"

    ],

    "transform": {

      "^.+\\.jsx?$": "babel-jest"

    },

    "globals": {

      "NODE_ENV": "test"

    }

  }

}

